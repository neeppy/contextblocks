{
  "name": "colorblocks",
  "displayName": "ColorBlocks",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "colorblocks.create-block-from-selection",
        "title": "Color Block: Highlight Selection"
      },
      {
        "command": "colorblocks.remove-blocks-from-selection",
        "title": "Color Block: Remove Blocks from Selection"
      },
      {
        "command": "colorblocks.remove-blocks-from-file",
        "title": "Color Block: Remove All Blocks from Current File"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "colorblocks.create-block-from-selection",
          "group": "navigation"
        },
        {
          "command": "colorblocks.remove-blocks-from-selection",
          "group": "navigation"
        },
        {
          "command": "colorblocks.remove-blocks-from-file",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
